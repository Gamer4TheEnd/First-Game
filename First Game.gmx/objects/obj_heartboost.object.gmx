            <string>///HeartBoost Player

//2 Hearts//
if (place_meeting(x,y,obj_player)) &amp;&amp; (lives &lt; 10)
{
    audio_play_sound(snd_coin,9,false)
    instance_create(x, y, obj_collect_heart);
    lives += 2
    instance_destroy()
}

//2 Hearts Max//
if (place_meeting(x,y,obj_player)) &amp;&amp; (lives = 10)
{
    audio_play_sound(snd_coin,9,false)
    instance_create(x, y, obj_collect_heart);
    lives += 2
    instance_destroy()
}


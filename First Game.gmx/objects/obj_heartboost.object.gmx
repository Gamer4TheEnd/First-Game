{
    audio_play_sound(snd_coin,9,false)
    instance_create(x, y, obj_collect_heart);
    lives += 1
    global.points += 25
    instance_destroy()
}

//0 Heart//
if (place_meeting(x,y,obj_player)) &amp;&amp; (lives = 12)
{
    audio_play_sound(snd_coin,9,false)
    instance_create(x, y, obj_collect_heart);
    global.points += 50
    instance_destroy()
}
